<%- include('./header.ejs')%>



<div class="container-fluid">
    <div class="row">

        <div class="col-sm-12 col-md-3 col-lg-2 p-0">
            <%- include('./left.ejs')%>
        </div>


        <div class="col-sm-12 col-md-9 col-lg-10 dash_">

            <% if(userregistration.length>0) { %>
            <table class="table border">
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Nname</th>
                    <th scope="col">Email</th>
                    <th scope="col">Password</th>
                    <th scope="col">phone no.</th>
                </tr>

                <% for(var i=0; i<userregistration.length; i++){%>
                <tr>
                    <td><%= i%></td>
                    <td><%= userregistration[i].fname %></td>
                    <td><%= userregistration[i].email %></td>
                    <td><%= userregistration[i].password %></td>
                    <td><%= userregistration[i].phone %></td>
                    <td>
                        <a href="/delete/<%= userregistration[i]._id %>">
                        <button class="btn text-danger bg-light" type="submit">Delete</button></a>
                    </td>
                    <td>
                        <a href="/edit/<%= userregistration[i]._id %>">
                        <button class="btn text-danger bg-light" type="submit">Edit</button></a>
                    </td>
                </tr>
                <% } %>
            </table>
            <% } else{ %>
                No Record Found
            <% } %>
            

        </div>
    </div>
</div>